style "Baroque-layout"    = "Baroque-default"
{
  bg[NORMAL]            = "#37383f"
    base[NORMAL]          = "#37383f"
    bg[ACTIVE]            = "#454f60"

    engine "pixmap"
    {
      image
      {
        function          = SHADOW
          detail            = "entry"
          state             = NORMAL
          file              = "shadow.png"
          recolorable       = TRUE
          border            = { 5, 5, 5, 5 }
        stretch           = TRUE
      }
      image
      {
        function          = FOCUS
          recolorable       = TRUE
          file              = "shadow.png"
          border            = { 5, 5, 5, 5 }
        stretch           = TRUE
      }
      image
      {
        function          = SHADOW
          state             = NORMAL
          file              = "shadow-evolution.png"
          recolorable       = TRUE
          border            = { 1, 1, 1, 1 }
        stretch           = TRUE
      }
      image
      {
        function          = BOX
          detail            = "button"
          state             = NORMAL
          file              = "button-out.png"
          recolorable       = TRUE
          border            = { 8, 8, 8, 8 }
        stretch           = TRUE
      }
    }
}

style "Baroque-drawingarea" = "Baroque-default"
{
  engine "pixmap"
  {
    image
    {
      function           = SHADOW
      file               = "evolution-highlight.png"
      border             = { 1, 1, 1, 1 }
      stretch            = TRUE
    }
    image
    {
      function           = BOX
      shadow             = IN
      detail             = "button"
      file               = "button-in.png"
      border             = { 8, 8, 8, 8 }
      stretch            = TRUE
    }
    image
    {
      function           = BOX
      detail             = "button"
      file               = "button-out.png"
      border             = { 8, 8, 8, 8 }
      stretch            = TRUE
    }
    image
    {
      function           = BOX
      file               = "none.png"
      border             = { 0, 0, 0, 0 }
      stretch            = TRUE
    }
  }
}

